{
   "name": "personal-api",
   "version": "0.6.2",
   "description": "",
   "author": "Wojciech Rola",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
      "migration:generate": "npm run build && npx typeorm-ts-node-commonjs migration:generate --dataSource dist/data-source.js ",
      "migration:run": "npm run build && npx typeorm-ts-node-commonjs migration:run --dataSource dist/data-source.js",
      "migration:revert": "npx typeorm-ts-node-commonjs migration:revert --dataSource dist/data-source.js"
   },
   "dependencies": {
      "@nestjs/axios": "^3.0.1",
      "@nestjs/common": "^10.2.10",
      "@nestjs/config": "^3.1.1",
      "@nestjs/core": "^10.2.10",
      "@nestjs/platform-express": "^10.2.10",
      "@nestjs/terminus": "^10.2.0",
      "@nestjs/typeorm": "^10.0.1",
      "@qdrant/js-client-rest": "^1.5.0",
      "axios": "^1.6.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "helmet": "^7.1.0",
      "langchain": "^0.0.199",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.2.0",
      "serpapi": "^2.0.0",
      "typeorm": "0.3.17",
      "uuid": "^9.0.1"
   },
   "devDependencies": {
      "@faker-js/faker": "^8.3.1",
      "@nestjs/cli": "^10.2.1",
      "@nestjs/schematics": "^10.0.3",
      "@nestjs/testing": "^10.2.10",
      "@types/express": "^4.17.13",
      "@types/jest": "^29.5.10",
      "@types/node": "^20.10.2",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^6.13.1",
      "@typescript-eslint/parser": "^6.13.1",
      "eslint": "^8.55.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "jest": "^29.7.0",
      "prettier": "^3.1.0",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "^29.1.1",
      "ts-loader": "^9.2.3",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.3.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
