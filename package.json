{
  "name": "personal-api",
  "version": "0.7.6",
  "description": "",
  "author": "Wojciech Rola",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "migration:generate": "npm run build && npx typeorm-ts-node-commonjs migration:generate --dataSource dist/data-source.js ",
    "migration:run": "npm run build && npx typeorm-ts-node-commonjs migration:run --dataSource dist/data-source.js",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert --dataSource dist/data-source.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@langchain/core": "^0.2.27",
    "@langchain/openai": "^0.1.3",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.3",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@qdrant/js-client-rest": "^1.9.0",
    "axios": "^1.7.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "langchain": "^0.2.5",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "typeorm": "0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "testcontainers": "^10.12.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
